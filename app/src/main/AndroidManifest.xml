<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ap.watchtogive"
    >

    <application
        android:name=".WatchToGiveApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.WatchToGive">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>

        <!--
            Real id: ca-app-pub-7049640721055375~2167672729
        -->


        <activity
            android:name="com.firebase.ui.auth.KickoffActivity"
            android:theme="@style/FirebaseAuthTheme"
            tools:ignore="Instantiatable"
            tools:replace="android:theme" />


        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.WatchToGive">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>